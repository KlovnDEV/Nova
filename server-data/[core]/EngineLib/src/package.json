{
  "name": "@nova/engine-lib",
  "version": "1.0.3",
  "description": "OOP FiveM Framework",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run client && npm run server && npm run copy",
    "client": "tsc -p ./client/",
    "server": "tsc -p ./server/",
    "clean": "rimraf ./lib/* && del /S /Q \".\\node_modules\\@nova\\engine-lib\\\" > nul",
    "copy": "xcopy /QEY \".\\lib\\*\" \".\\node_modules\\@nova\\engine-lib\\\"",
    "test": "jest shared/Math/__tests__/Random.ts",
    "lint": "eslint --debug ./client ./server ./shared",
    "lint:write": "eslint --fix  ./client ./server ./shared",
    "rollup": "rollup -c",
    "pack": "npm pack --workspace=lib"
  },
  "files": [
    "lib/**/*"
  ],
  "workspaces": [
    "lib"
  ],
  "author": "https://github.com/Sadler2, https://github.com/mderkach",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.4",
    "@rollup/plugin-commonjs": "^19.0.1",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.2",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "cpy-cli": "^3.1.1",
    "babel-jest": "^27.0.6",
    "dts-bundle-webpack": "^1.0.2",
    "esbuild-loader": "^2.13.1",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-airbnb-typescript-prettier": "^4.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-module-utils": "^2.6.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.54.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-styles": "^3.14.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.3.5",
    "typescript-transform-paths": "^3.1.0",
    "webpack": "^5.46.0",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "@citizenfx/client": "^2.0.4269-1",
    "@citizenfx/server": "^2.0.4269-1"
  }
