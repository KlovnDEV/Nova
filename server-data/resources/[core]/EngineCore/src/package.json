{
  "name": "engine-core",
  "version": "1.0.0",
  "description": "OOP FiveM Framework",
  "main": "./index.ts",
  "scripts": {
    "production": "rimraf ../dist && npm run client-precompile && npm run server-precompile && npm run client && npm run server && npm run clean",
    "client": "webpack --config ./webpack.config.js --progress --env=entry=client",
    "client-precompile": "cd %INIT_CWD% && cd ./client && tsc",
    "server": "webpack --config ./webpack.config.js --progress --env=entry=server",
    "server-precompile": "cd %INIT_CWD% && cd ./server && tsc",
    "test": "jest",
    "lint": "eslint --debug ./client ./server ./shared",
    "lint:write": "eslint --fix  ./client ./server ./shared",
    "clean": "cd %INIT_CWD% && rimraf ./compiled"
  },
  "author": "https://github.com/Sadler2, https://github.com/mderkach",
  "license": "ISC",
  "devDependencies": {
    "@types/events": "^3.0.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.1",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "babel-jest": "^27.0.6",
    "dts-bundle-webpack": "^1.0.2",
    "esbuild-loader": "^2.13.1",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-airbnb-typescript-prettier": "^4.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-module-utils": "^2.6.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.3.5",
    "typescript-transform-paths": "^3.1.0",
    "webpack": "^5.46.0",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "@citizenfx/client": "^2.0.4261-1",
    "@citizenfx/server": "^2.0.4261-1",
    "@nova/engine-lib": "file:./nova-engine-lib-1.0.3.tgz",
    "events": "^3.3.0"
  }
}
